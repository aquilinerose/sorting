#include <stdio.h>
void main()
{
  int n, array[1000],i,j, t, flag = 0;

  printf("Enter number of elements\n");
  scanf("%d", &n);

  printf("Enter the integers\n");

  for (i= 0;i < n;i++)
    scanf("%d", &array[i]);

  for (i = 1 ; i <= n - 1;i++) 
  {
    t = array[i];

    for (j = i - 1 ; j >= 0; j--) 
    {
      if (array[j] > t) 
      {
        array[j+1] = array[j];
        flag = 1;
      }
      else
        break;
    }
    if (flag)
      array[j+1] = t;
  }

  printf("Sorted list in ascending order:\n");

  for (i = 0;i<= n - 1;i++) 
  {
    printf("%d\t", array[i]);
  }
}
